<div>
  {{#with main}}
    <main>
        {{> title data="My educations" big=true}}
      <section>
        <ul>
          {{#each educations}}
            <li>
              {{name}}
              {{#unless isEnd}} (ще не закінчив) {{/unless}}
              {{#if isEnd}} (закінченно) {{/if}}
            </li>
          {{/each}}
        </ul>
      </section>
      <section>
        {{> title data="My certificates" big=false}}
        <ul>
            {{#each certificates}}
            <li>
                {{name}} ({{id}})
            </li>
            {{/each}}
        </ul>
      </section>
    </main>
  {{/with}}
</div>